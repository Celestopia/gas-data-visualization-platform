[*.{js,jsx,mjs,cjs,ts,tsx,mts,cts,vue}] # appliy to specified file types
charset = utf-8 # set the character encoding to UTF-8
indent_size = 2 # use 2 spaces for indentation
indent_style = space # use spaces instead of tabs for indentation
insert_final_newline = true # add a new line at the end of the file
trim_trailing_whitespace = true # remove trailing whitespace at the end of lines
